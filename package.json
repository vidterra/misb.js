	],
	"author": "Dillon Bussert",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/vidterra/misb.js/issues"
	},
	"homepage": "https://github.com/vidterra/misb.js#readme",
	"devDependencies": {
		"jest": "^29.7.0",
		"prettier": "^3.0.3"
	},
	"prettier": {
		"printWidth": 90,
		"tabWidth": 4,
		"useTabs": true,
